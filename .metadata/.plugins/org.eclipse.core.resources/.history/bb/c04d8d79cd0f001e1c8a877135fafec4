/*
  Faça um programa que determine e mostre os cincos primeiros multiplos de 3, 
  considerando números maiores que zero.
 */
public class Exercico01 {

	public static void main(String[] args) {
		
		int num = 1;
		
		
		while(num >  0){
			
			
			for( num = 1; num <3; num++){
				int cont;
				cont = num * 3;
				 System.out.println(num * cont);
			}
		} 

	}

}
